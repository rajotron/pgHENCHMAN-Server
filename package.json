{
  "name": "pgHENCHMAN",
  "version": "1.0.0",
  "license": "GPL 2.0",
  "description": "pgHENCHMAN backend Server",
  "engines": {
    "node": "8.11.1",
    "npm": "6.4.0"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "webui": "ng serve --proxy-config proxy.conf.json --open",
    "backend": "concurrently \"tsc -w -p server\" \"node --max-old-space-size=4096 dist/server/app.js\"",
    "dev": "concurrently \"mongod\" \"ng serve --proxy-config proxy.conf.json --open\" \"tsc -w -p server\" \"node dist/server/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build --aot --prod && tsc -p server && node dist/server/app.js\""
  },
  "private": true,
